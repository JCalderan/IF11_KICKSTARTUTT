<div class="projectItem_container span12">
    <div class="projectItem_header row-fluid">
        <div class="span12">
            <h4><strong><a href="#" class="toResProject">{{nom_projet}}</a></strong></h4>
        </div>
    </div>
    <div class="projectItem_body row-fluid">
        <div class="span3">
            <a href="#" class="toResProject" class="thumbnail"><img src="http://placehold.it/260x180" alt="ALT NAME"/></a>
        </div>
        <div class="span6">
            <p>
                {{description_projet}}...
            </p>
            <a href="#" class="toResProject">Read More</a>
        </div>
        <div class="span3">
            <p><i class="icon-tags"></i> Tags :</p>
            <p>
                {{#each topics}}
                    <a href="#" class="toResTags" ><span class="label label-info">{{this}}</span></a> 
                {{/each}}
            </p>
        </div>
    </div>
    <div class="projectItem_footer row-fluid">
        <div class="span12">
            <p></p>
            <p>
                <i class="icon-user"></i> by <a href="#">
                    {{#each members}}
                        {{#if this.fondateur}}
                            {{this.fondateur.ref_utilisateur}}
                        {{/if}}
                    {{/each}}
                </a> 
                | <i class="icon-calendar"></i> {{date_creation}}
                | <i class="icon-comment"></i> <a href="#">N Comments</a>
                | <i class="icon-pencil"></i> <a href="#">N Contributeurs</a>
                | <i class="icon-star"></i> Note :
                <span class="star_rating"></span>
              </p>
        </div>
    </div>
    <hr />
</div>
