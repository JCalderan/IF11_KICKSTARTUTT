<div class="thumbnail" style="height: 350px;">
    <div class="row-fluid" style="margin-bottom: 3%;">
        <div class="span12" style="
                                color: #FFFFFF;border: 1px solid #D4D4D4;
                                border-radius: 4px 4px 4px 4px;
                                box-shadow: 0 1px 4px rgba(0, 0, 0, 0.067);
                                background-color: {{theme_color}};
                                #background-image: linear-gradient(to bottom, #F2F2F2, #CC4400);
                                background-repeat: repeat-x;
                                padding-top: 5px;
        ">
            <p class="text-center lead" style="margin: 0px;">{{title}}</p>
        </div>
    </div>
    <div class="row-fluid" style="height: 80%; overflow-x: hidden; overflow-y: auto; padding-left: 5px; padding-right: 5px;">
        {{#each articles}}
        <div class="span12 thumbnail" style="height: 125px; margin: 0px;">
            <div class="span4">
                <img src="{{this.img}}" alt="ALT NAME"/>
            </div>
            <div class="caption span6">
                <p >{{this.resume}}</p>
            </div>
            <div class="span2" style="height: 100%; position: relative;">
                <a href="{{this.link}}" class="btn btn-block btn-primary" type="button" style="position: absolute; top: 50%; margin-top: -15px; height: 30px;"> OK </a>
            </div>
        </div>
        <div class="span12" ></div>
        {{/each}}        
    </div>
    <div class="row-fluid">
        <div class="span12">
        </div>
    </div>
</div>
    
    
    
    
